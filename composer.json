{
    "name": "rachellawson/blog",
    "description": "My blog",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2",
        "cweagans/composer-patches": "^1.7",
        "drupal/activitypub": "^1.0@alpha",
        "drupal/admin_toolbar": "^3.0",
        "drupal/admin_toolbar_content": "^2",
        "drupal/autosave_form": "^1.2",
        "drupal/colorbox": "^2",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "^4.0",
        "drupal/diff": "^1.0",
        "drupal/environment_indicator": "^4.0",
        "drupal/geocoder": "^4.4",
        "drupal/geofield": "^1.15",
        "drupal/geofield_map": "^3.0",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/honeypot": "^2.0",
        "drupal/image_widget_crop": "^2.3",
        "drupal/leaflet": "^10.0",
        "drupal/leaflet_more_maps": "^2.1",
        "drupal/media_entity_twitter": "^2.7",
        "drupal/metatag": "^2",
        "drupal/noreqnewpass": "^1.2",
        "drupal/openid_connect": "^3",
        "drupal/pathauto": "^1.8",
        "drupal/reader": "^1.0@beta",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.4",
        "drupal/schema_metatag": "^3",
        "drupal/upgrade_status": "^4.3",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "^13",
        "platformsh/config-reader": "^2.3"
    },
    "require-dev": {
        "behat/mink-browserkit-driver": "^2.2",
        "drupal/core-dev": "^10",
        "drupal/drupal-extension": "^5.0",
        "drupal/stage_file_proxy": "^2.1",
        "palantirnet/drupal-rector": "^0.20.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "violinist": {
            "commit_message_convention": "conventional",
            "allow_update_indirect_with_direct": 0,
            "allow_updates_beyond_constraint": 1,
            "always_update_all": 1,
            "assignees": ["rachellawson"],
            "automerge": 0,
            "automerge_security": 0,
            "check_only_direct_dependencies": 1,
            "number_of_concurrent_updates": 0,
            "one_pull_request_per_package": 1,
            "run_scripts": 1,
            "update_with_dependencies": 1
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
